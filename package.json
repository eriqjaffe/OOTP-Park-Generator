{
  "name": "parkgen",
  "version": "1.0.0",
  "description": "OOTP Park Generator",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "keywords": [
    "OOTP",
    "Park",
    "Generator"
  ],
  "author": "Eriq Jaffe",
  "license": "ISC",
  "dependencies": {
    "archiver": "^5.3.0",
    "electron": "^16.0.1",
    "electron-build": "^0.0.3",
    "express": "^4.17.1",
    "jimp": "^0.16.1",
    "sql.js": "^1.6.2",
    "xml-escape": "^1.1.0",
    "xmlbuilder2": "^3.0.2"
  },
  "build": {
    "appId": "com.hypermodules.testapp",
    "mac": {
      "category": "public.app-category.utilities",
      "target": [
        "zip",
        "dmg"
      ]
    },
    "portable": {
      "artifactName": "${productName}_${version}_portable.${ext}"
    },
    "nsis": {
      "perMachine": true,
      "artifactName": "${productName}_${version}_setup.${ext}"
    },
    "linux": {
      "target": [
        "deb",
        "rpm",
        "tar.gz"
      ]
    },
    "deb": {}
  }
}
